	journal = {Magnetic Resonance Imaging},
	author = {Torfeh, Tarraf and Hammoud, Rabih and Perkins, Gregory and McGarry, Maeve and Aouadi, Souha and Celik, Azim and Hwang, Ken-Pin and Stancanello, Joseph and Petric, Primoz and Al-Hammadi, Noora},
	month = jun,
	year = {2016},
	keywords = {Magnetic resonance imaging, Geometric distortion, Quality control, Software tools},
	pages = {645--653},
	file = {ScienceDirect Snapshot:/home/brendan/Zotero/storage/SLE3WS9F/S0730725X16000023.html:text/html},
